  "license": "MIT",
  "private": true,
  "scripts": {
    "build:bg": "parcel build src/background/index.ts --dist-dir dist/background",
    "build:options": "parcel build src/options/index.html --dist-dir dist/options --public-url ./",
    "build:newtab": "parcel build src/newtab/index.html --dist-dir dist/newtab --public-url ./",
    "copy:static": "copyfiles manifest.json assets/**/* assets/* dist",
    "build": "run-s build:newtab build:bg",
    "clean": "rimraf dist",
    "dist": "run-s clean build copy:static"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.117",
    "@types/node": "^14.0.14",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "@types/rebass": "^4.0.6",
    "@types/rebass__forms": "^4.0.2",
    "copyfiles": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.0.0-beta.1",
    "rimraf": "^3.0.2",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
    "@fortawesome/free-brands-svg-icons": "^5.13.1",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@rebass/forms": "^4.0.6",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "rebass": "^4.0.7",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "targets": {
    "newtab": {
      "context": "browser",
      "sourceMap": false
    }
  }
